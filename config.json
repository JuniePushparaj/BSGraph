{
    "inputFile": "input\\inputPS2.txt",
    "promptsFile": "input\\promptsPS2.txt",
    "outputFile": "output\\outputPS2.txt",
    "formatter": {
        "symbol":"-",
        "count": "75"
    },
    "searchFunc": {
        "searchActor": {
            "param" : 1,
            "callingFunc": "displayMoviesOfActor"
        },
        "searchMovie": {
            "param" : 1,
            "callingFunc": "displayActorsOfMovie"
        },
        "RMovies": {
            "param" : 2,
            "callingFunc": "findMovieRelation"
        },
        "TMovies":{
            "param" : 2,
            "callingFunc": "findMovieTransRelation"
        }
    },
    "operationAllowed": ["searchActor", "searchMovie", "RMovies", "TMovies" ]
}